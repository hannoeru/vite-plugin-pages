{
  "name": "vite-plugin-pages",
  "version": "0.23.0",
  "packageManager": "pnpm@7.0.1",
  "description": "File system base vue-router plugin for Vite",
  "keywords": [
    "vite",
    "vue",
    "vute-router",
    "react",
    "react-router",
    "file-system-based",
    "routing"
  ],
  "license": "MIT",
  "author": "hannoeru <me@hanlee.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hannoeru/vite-plugin-pages"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./client-react": {
      "types": "./client-react.d.ts"
    },
    "./client-solid": {
      "types": "./client-solid.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts",
    "client-react.d.ts",
    "client-solid.d.ts",
    "LICENSE"
  ],
  "homepage": "https://github.com/hannoeru/vite-plugin-pages",
  "bugs": "https://github.com/hannoeru/vite-plugin-pages/issues",
  "scripts": {
    "dev": "npm run build -- --watch --ignore-watch examples",
    "build": "tsup",
    "dev:vue": "pnpm --filter vue dev",
    "dev:react": "pnpm --filter react dev",
    "build:vue": "pnpm --filter vue build",
    "build:react": "pnpm --filter react build",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "publish:ci": "esno scripts/publish.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.0",
    "vite": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "debug": "^4.3.4",
    "deep-equal": "^2.0.5",
    "fast-glob": "^3.2.11",
    "json5": "^2.2.1",
    "local-pkg": "^0.4.1",
    "picocolors": "^1.0.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@antfu/utils": "^0.5.1",
    "@hannoeru/eslint-config": "^0.8.5",
    "@types/deep-equal": "^1.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/node": "^17.0.31",
    "@vitest/ui": "^0.12.0",
    "@vue/compiler-sfc": "^3.2.33",
    "bumpp": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "esno": "^0.14.1",
    "playwright": "^1.21.1",
    "react": "^18.0.0",
    "react-router": "^6.3.0",
    "solid-app-router": "^0.3.2",
    "solid-js": "^1.3.17",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.12.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  }
}
